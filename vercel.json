{
  "version": 2,
  "routes": [
    {
      "src": "^/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/dashboard$",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "^/$",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "^/[^/]+$",
      "dest": "/",
      "status": 301
    },
    {
      "src": ".*",
      "dest": "/",
      "status": 301
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}